@model IEnumerable<AdventuresWebApp.Models.AwbuildVersion>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DatabaseVersion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.VersionDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ModifiedDate)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DatabaseVersion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VersionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SystemInformationId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SystemInformationId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SystemInformationId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
