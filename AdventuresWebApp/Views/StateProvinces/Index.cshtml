@model IEnumerable<AdventuresWebApp.Models.StateProvince>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.StateProvinceCode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsOnlyStateProvinceFlag)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Rowguid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ModifiedDate)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.CountryRegionCodeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Territory)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StateProvinceCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsOnlyStateProvinceFlag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rowguid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountryRegionCodeNavigation.CountryRegionCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Territory.CountryRegionCode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StateProvinceId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StateProvinceId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StateProvinceId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
